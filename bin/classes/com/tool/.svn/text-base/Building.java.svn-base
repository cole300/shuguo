package com.tool;

public class Building {

	/*建筑名称name*/
	private String name;
	/*建筑图片id*/
	private int id_pic;
	/*建筑在地图上坐标*/
	private int verId_map;
	private int horId_map;
	/*建筑类型<DaTing><BingYing><TieJiangPu><JiuGuan>*/
	private int type;
	/*建筑图块宽度*/
	private int w_tile;
	/*建筑图块高度*/
	private int h_tile;
	
	public Building()
	{
		
	}

	/*getter and setter*/
	public String getName(){
		return this.name;
	}
	
	public void setName(String str){
		this.name = str;
	}
	
	public int getId_pic() {
		return id_pic;
	}

	public void setId_pic(int id_pic) {
		this.id_pic = id_pic;
	}

	public void setVerId_map(int value)
	{
		this.verId_map = value;
	}
	
	public int getVerId_map()
	{
		return this.verId_map;
	}
	
	public void setHorId_map(int value){
		this.horId_map = value;
	}

	public int getHorId_map()
	{
		return this.horId_map;
	}
	
	public int getType() {
		return type;
	}

	public void setType(int type) {
		this.type = type;
	}

	public int getW_tile() {
		return w_tile;	
	}

	public void setW_tile(int w_tile) {
		this.w_tile = w_tile;
	}

	public int getH_tile() {
		return h_tile;
	}

	public void setH_tile(int h_tile) {
		this.h_tile = h_tile;
	}
	
}
